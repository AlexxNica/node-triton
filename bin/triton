var p = console.log;
var cmdln = require('cmdln');
var CLI = require('../lib/cli');

if (require.main === module) {
    var cli = new CLI();
    cmdln.main(cli, {
        argv: process.argv,
        showCode: true,
        showNoCommandErr: false
    });
}
